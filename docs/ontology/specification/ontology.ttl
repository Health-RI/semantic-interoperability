@prefix : <https://w3id.org/health-ri/ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <https://w3id.org/health-ri/ontology#> .

<https://w3id.org/health-ri/ontology#> rdf:type owl:Ontology ;
                                        owl:imports <http://purl.org/nemo/gufo#> ;
                                        <http://purl.org/dc/terms/abstract> "The Health-RI Ontology defines core concepts relevant to representing personal sex and gender information in health records. It provides a domain ontology for consistent description, data exchange, and integration across systems, covering administrative/legal, biological, and social aspects of sex and gender."@en ;
                                        <http://purl.org/dc/terms/creator> <https://orcid.org/0000-0003-2736-7817> ;
                                        <http://purl.org/dc/terms/description> "This ontology models sex and gender concepts relevant to health records in the Netherlands. It includes classes for Administrative Gender, Legal Gender, Sex Assigned at Birth, Phenotypic Sex, Karyotypical Sex, and Gender Identity, among others. The ontology enables consistent terminology and semantic interoperability across health information systems."@en ;
                                        <http://purl.org/dc/terms/identifier> "https://w3id.org/health-ri/ontology" ;
                                        <http://purl.org/dc/terms/issued> "2024-06-18"^^xsd:date ;
                                        <http://purl.org/dc/terms/language> "en"^^xsd:language ;
                                        <http://purl.org/dc/terms/license> <https://creativecommons.org/licenses/by/4.0/> ;
                                        <http://purl.org/dc/terms/modified> "2025-01-02"^^xsd:date ;
                                        <http://purl.org/dc/terms/publisher> <https://w3id.org/health-ri/publisher/health-ri> ;
                                        <http://purl.org/dc/terms/title> "Health-RI Ontology for Sex and Gender Concepts"@en ;
                                        <http://purl.org/vocab/vann/preferredNamespacePrefix> "health-ri" ;
                                        <http://purl.org/vocab/vann/preferredNamespaceUri> "https://w3id.org/health-ri/ontology#"^^xsd:anyURI ;
                                        owl:versionInfo "0.5.2 â€“ Includes updated definitions and improved structure for Administrative, Legal, and Phenotypic Sex classes, plus enhanced metadata."@en ;
                                        <http://www.w3.org/ns/dcat#keyword> "health records"@en ,
                                                                            "ontology"@en ,
                                                                            "semantic interoperability"@en ,
                                                                            "sex and gender"@en ;
                                        <http://www.w3.org/ns/dcat#landingPage> <https://w3id.org/health-ri/ontology> ;
                                        <http://www.w3.org/ns/dcat#version> "0.5.2" ;
                                        <http://xmlns.com/foaf/0.1/homepage> <https://w3id.org/health-ri/ontology> ;
                                        <http://xmlns.com/foaf/0.1/logo> <https://w3id.org/health-ri/resources/logo.png> ;
                                        <https://w3id.org/mod#repository> <https://github.com/Health-RI/semantic-interoperability> .

