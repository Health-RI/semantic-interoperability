     xmlns:ontology="https://w3id.org/health-ri/ontology#">
    <owl:Ontology rdf:about="https://w3id.org/health-ri/ontology#">
        <owl:imports rdf:resource="http://purl.org/nemo/gufo#"/>
        <terms:abstract xml:lang="en">The Health-RI Ontology defines core concepts relevant to representing personal sex and gender information in health records. It provides a domain ontology for consistent description, data exchange, and integration across systems, covering administrative/legal, biological, and social aspects of sex and gender.</terms:abstract>
        <terms:creator rdf:resource="https://orcid.org/0000-0003-2736-7817"/>
        <terms:description xml:lang="en">This ontology models sex and gender concepts relevant to health records in the Netherlands. It includes classes for Administrative Gender, Legal Gender, Sex Assigned at Birth, Phenotypic Sex, Karyotypical Sex, and Gender Identity, among others. The ontology enables consistent terminology and semantic interoperability across health information systems.</terms:description>
        <terms:identifier>https://w3id.org/health-ri/ontology</terms:identifier>
        <terms:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2024-06-18</terms:issued>
        <terms:language rdf:datatype="http://www.w3.org/2001/XMLSchema#language">en</terms:language>
        <terms:license rdf:resource="https://creativecommons.org/licenses/by/4.0/"/>
        <terms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2025-01-02</terms:modified>
        <terms:publisher rdf:resource="https://w3id.org/health-ri/publisher/health-ri"/>
        <terms:title xml:lang="en">Health-RI Ontology for Sex and Gender Concepts</terms:title>
        <vann:preferredNamespacePrefix>health-ri</vann:preferredNamespacePrefix>
        <vann:preferredNamespaceUri rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">https://w3id.org/health-ri/ontology#</vann:preferredNamespaceUri>
        <owl:versionInfo xml:lang="en">0.5.2 â€“ Includes updated definitions and improved structure for Administrative, Legal, and Phenotypic Sex classes, plus enhanced metadata.</owl:versionInfo>
        <dcat:keyword xml:lang="en">health records</dcat:keyword>
        <dcat:keyword xml:lang="en">ontology</dcat:keyword>
        <dcat:keyword xml:lang="en">semantic interoperability</dcat:keyword>
        <dcat:keyword xml:lang="en">sex and gender</dcat:keyword>
        <dcat:landingPage rdf:resource="https://w3id.org/health-ri/ontology"/>
        <dcat:version>0.5.2</dcat:version>
        <foaf:homepage rdf:resource="https://w3id.org/health-ri/ontology"/>
        <foaf:logo rdf:resource="https://w3id.org/health-ri/resources/logo.png"/>
        <mod:repository rdf:resource="https://github.com/Health-RI/semantic-interoperability"/>
    </owl:Ontology>
    


