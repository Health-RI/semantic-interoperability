<https://w3id.org/health-ri/ontology#> rdf:type owl:Ontology ;
    dct:abstract """The Health-RI Ontology defines core concepts relevant to representing personal sex and gender information in health records. It provides a domain ontology for consistent description, data exchange, and integration across systems, covering administrative/legal, biological, and social aspects of sex and gender."""@en ;
    dct:creator <https://orcid.org/0000-0003-2736-7817> ;
    dct:description """This ontology models sex and gender concepts relevant to health records in the Netherlands. It includes classes for Administrative Gender, Legal Gender, Sex Assigned at Birth, Phenotypic Sex, Karyotypical Sex, and Gender Identity, among others. The ontology enables consistent terminology and semantic interoperability across health information systems."""@en ;
    dct:identifier "https://w3id.org/health-ri/ontology" ;
    dct:issued "2024-06-18"^^xsd:date ;
    dct:language "en"^^xsd:language ;
    dct:license <https://creativecommons.org/licenses/by/4.0/> ;
    dct:modified "2025-01-02"^^xsd:date ;
    dct:publisher <https://w3id.org/health-ri/publisher/health-ri> ;
    dct:title "Health-RI Ontology for Sex and Gender Concepts"@en ;
    vann:preferredNamespacePrefix "health-ri" ;
    vann:preferredNamespaceUri "https://w3id.org/health-ri/ontology#"^^xsd:anyURI ;
    owl:versionInfo "0.5.2 â€“ Includes updated definitions and improved structure for Administrative, Legal, and Phenotypic Sex classes, plus enhanced metadata."@en ;
    dcat:keyword "sex and gender"@en ,
                 "health records"@en ,
                 "ontology"@en ,
                 "semantic interoperability"@en ;
    dcat:landingPage <https://w3id.org/health-ri/ontology> ;
    dcat:version "0.5.2" ;
    foaf:homepage <https://w3id.org/health-ri/ontology> ;
    foaf:logo <https://w3id.org/health-ri/resources/logo.png> ;
    mod:repository <https://github.com/Health-RI/semantic-interoperability> .


<https://w3id.org/health-ri/ontology> rdf:type owl:Ontology ;
                                       owl:imports gufo: .

#################################################################
#    Annotation properties
#################################################################

###  https://w3id.org/health-ri/ontology#administrativeGenderInheresInPersonWithRecognizedAdministrativeGender
health-ri:administrativeGenderInheresInPersonWithRecognizedAdministrativeGender rdf:type owl:AnnotationProperty .


###  https://w3id.org/health-ri/ontology#administrativeGenderRecognitionMediatesAdministrativeGenderRecognizer
health-ri:administrativeGenderRecognitionMediatesAdministrativeGenderRecognizer rdf:type owl:AnnotationProperty .


###  https://w3id.org/health-ri/ontology#administrativeGenderRecognitionMediatesPersonWithRecognizedAdministrativeGender
health-ri:administrativeGenderRecognitionMediatesPersonWithRecognizedAdministrativeGender rdf:type owl:AnnotationProperty .


###  https://w3id.org/health-ri/ontology#legalGenderRecognitionMediatesLegalGenderRecognizer
health-ri:legalGenderRecognitionMediatesLegalGenderRecognizer rdf:type owl:AnnotationProperty ;
                                                              rdfs:subPropertyOf health-ri:administrativeGenderRecognitionMediatesAdministrativeGenderRecognizer .


###  https://w3id.org/health-ri/ontology#legalGenderRecognitionMediatesPersonWithRecognizedLegalGender
health-ri:legalGenderRecognitionMediatesPersonWithRecognizedLegalGender rdf:type owl:AnnotationProperty ;
                                                                        rdfs:subPropertyOf health-ri:administrativeGenderRecognitionMediatesPersonWithRecognizedAdministrativeGender .


###  https://w3id.org/health-ri/ontology#personWithRecognizedLegalGenderInheresInLegalGender
health-ri:personWithRecognizedLegalGenderInheresInLegalGender rdf:type owl:AnnotationProperty ;
                                                              rdfs:subPropertyOf health-ri:administrativeGenderInheresInPersonWithRecognizedAdministrativeGender .


#################################################################
#    Classes
#################################################################

###  https://w3id.org/health-ri/ontology#AdministrativeGender
health-ri:AdministrativeGender rdf:type owl:Class ;
                               rdfs:subClassOf gufo:ExtrinsicMode ,
                                               health-ri:Gender ,
                                               [ rdf:type owl:Restriction ;
                                                 owl:onProperty gufo:inheresIn ;
                                                 owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                 owl:onClass health-ri:PersonWithRecognizedAdministrativeGender
                                               ] ;
                               owl:disjointWith health-ri:GenderIdentity .


###  https://w3id.org/health-ri/ontology#AdministrativeGenderRecognition
health-ri:AdministrativeGenderRecognition rdf:type owl:Class ;
                                          rdfs:subClassOf gufo:Relator ,
                                                          [ rdf:type owl:Restriction ;
                                                            owl:onProperty gufo:mediates ;
                                                            owl:someValuesFrom health-ri:PersonWithRecognizedAdministrativeGender
                                                          ] ,
                                                          [ rdf:type owl:Restriction ;
                                                            owl:onProperty gufo:mediates ;
                                                            owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                            owl:onClass health-ri:AdministrativeGenderRecognizer
                                                          ] .


###  https://w3id.org/health-ri/ontology#AdministrativeGenderRecognizer
health-ri:AdministrativeGenderRecognizer rdf:type owl:Class ;
                                         rdfs:subClassOf health-ri:Organization ,
                                                         [ rdf:type owl:Restriction ;
                                                           owl:onProperty [ owl:inverseOf gufo:mediates
                                                                          ] ;
                                                           owl:someValuesFrom health-ri:AdministrativeGenderRecognition
                                                         ] .


###  https://w3id.org/health-ri/ontology#Allosome
health-ri:Allosome rdf:type owl:Class ;
                   owl:equivalentClass [ rdf:type owl:Class ;
                                         owl:unionOf ( health-ri:AllosomeX
                                                       health-ri:AllosomeY
                                                     )
                                       ] ;
                   rdfs:subClassOf health-ri:Chromosome .


###  https://w3id.org/health-ri/ontology#AllosomeX
health-ri:AllosomeX rdf:type owl:Class ;
                    rdfs:subClassOf health-ri:Allosome ;
                    owl:disjointWith health-ri:AllosomeY .


###  https://w3id.org/health-ri/ontology#AllosomeY
health-ri:AllosomeY rdf:type owl:Class ;
                    rdfs:subClassOf health-ri:Allosome .


###  https://w3id.org/health-ri/ontology#AmbiguousPhenotypicPerson
health-ri:AmbiguousPhenotypicPerson rdf:type owl:Class ;
                                    rdfs:subClassOf health-ri:PersonWithAssessedPhenotypicSex .


###  https://w3id.org/health-ri/ontology#BirthNotification
health-ri:BirthNotification rdf:type owl:Class ;
                            rdfs:subClassOf gufo:FunctionalComplex ,
                                            [ rdf:type owl:Restriction ;
                                              owl:onProperty [ owl:inverseOf gufo:mediates
                                                             ] ;
                                              owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                              owl:onClass health-ri:SexAtBirthAssignment
                                            ] .


###  https://w3id.org/health-ri/ontology#BirthRelatedLegislationRule
health-ri:BirthRelatedLegislationRule rdf:type owl:Class ;
                                      rdfs:subClassOf gufo:IntrinsicMode .


###  https://w3id.org/health-ri/ontology#Chromosome
health-ri:Chromosome rdf:type owl:Class ;
                     rdfs:subClassOf gufo:FunctionalComplex .


###  https://w3id.org/health-ri/ontology#DiploidCell
health-ri:DiploidCell rdf:type owl:Class ;
                      rdfs:subClassOf health-ri:HumanCell ;
                      owl:disjointWith health-ri:HaploidCell .


###  https://w3id.org/health-ri/ontology#Document
health-ri:Document rdf:type owl:Class ;
                   rdfs:subClassOf gufo:FunctionalComplex .


###  https://w3id.org/health-ri/ontology#FemaleBiologicalPerson
health-ri:FemaleBiologicalPerson rdf:type owl:Class ;
                                 rdfs:subClassOf health-ri:Person .


###  https://w3id.org/health-ri/ontology#FemaleGenderIdentity
health-ri:FemaleGenderIdentity rdf:type owl:Class ;
                               rdfs:subClassOf health-ri:GenderIdentity .


###  https://w3id.org/health-ri/ontology#FemalePhenotypicPerson
health-ri:FemalePhenotypicPerson rdf:type owl:Class ;
                                 rdfs:subClassOf health-ri:PersonWithAssessedPhenotypicSex .


###  https://w3id.org/health-ri/ontology#FemaleTypicalDimorphicCharacteristic
health-ri:FemaleTypicalDimorphicCharacteristic rdf:type owl:Class ;
                                               rdfs:subClassOf health-ri:SexualDimorphicCharacteristic .


###  https://w3id.org/health-ri/ontology#FemaleTypicalVisualSexCharacteristic
health-ri:FemaleTypicalVisualSexCharacteristic rdf:type owl:Class ;
                                               rdfs:subClassOf health-ri:VisualSexCharacteristic .


###  https://w3id.org/health-ri/ontology#Gamete
health-ri:Gamete rdf:type owl:Class ;
                 rdfs:subClassOf health-ri:HaploidCell .


###  https://w3id.org/health-ri/ontology#Gender
health-ri:Gender rdf:type owl:Class ;
                 rdfs:subClassOf gufo:Aspect ,
                                 [ rdf:type owl:Class ;
                                   owl:unionOf ( gufo:ExtrinsicMode
                                                 gufo:IntrinsicMode
                                               )
                                 ] .


###  https://w3id.org/health-ri/ontology#GenderIdentity
health-ri:GenderIdentity rdf:type owl:Class ;
                         owl:equivalentClass [ rdf:type owl:Class ;
                                               owl:unionOf ( health-ri:FemaleGenderIdentity
                                                             health-ri:MaleGenderIdentity
                                                             health-ri:NonBinaryGenderIdentity
                                                           )
                                             ] ;
                         rdfs:subClassOf gufo:IntrinsicMode ,
                                         health-ri:Gender ,
                                         [ rdf:type owl:Restriction ;
                                           owl:onProperty gufo:inheresIn ;
                                           owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                           owl:onClass health-ri:Person
                                         ] .


###  https://w3id.org/health-ri/ontology#GenderRecognitionDocument
health-ri:GenderRecognitionDocument rdf:type owl:Class ;
                                    rdfs:subClassOf health-ri:Document ,
                                                    [ rdf:type owl:Restriction ;
                                                      owl:onProperty [ owl:inverseOf gufo:mediates
                                                                     ] ;
                                                      owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                      owl:onClass health-ri:LegalGenderRecognition
                                                    ] .


###  https://w3id.org/health-ri/ontology#Government
health-ri:Government rdf:type owl:Class ;
                     rdfs:subClassOf health-ri:Organization .


###  https://w3id.org/health-ri/ontology#HaploidCell
health-ri:HaploidCell rdf:type owl:Class ;
                      rdfs:subClassOf health-ri:HumanCell .


###  https://w3id.org/health-ri/ontology#HealthcareProfessional
health-ri:HealthcareProfessional rdf:type owl:Class ;
                                 rdfs:subClassOf health-ri:Person .


###  https://w3id.org/health-ri/ontology#HumanCell
health-ri:HumanCell rdf:type owl:Class ;
                    owl:equivalentClass [ rdf:type owl:Class ;
                                          owl:unionOf ( health-ri:DiploidCell
                                                        health-ri:HaploidCell
                                                      )
                                        ] ;
                    rdfs:subClassOf gufo:FunctionalComplex .


###  https://w3id.org/health-ri/ontology#IndeterminateBiologicalPerson
health-ri:IndeterminateBiologicalPerson rdf:type owl:Class ;
                                        rdfs:subClassOf health-ri:Person .


###  https://w3id.org/health-ri/ontology#IndeterminateDimorphicCharacteristic
health-ri:IndeterminateDimorphicCharacteristic rdf:type owl:Class ;
                                               rdfs:subClassOf health-ri:SexualDimorphicCharacteristic .


###  https://w3id.org/health-ri/ontology#IndeterminateVisualSexCharacteristic
health-ri:IndeterminateVisualSexCharacteristic rdf:type owl:Class ;
                                               rdfs:subClassOf health-ri:VisualSexCharacteristic .


###  https://w3id.org/health-ri/ontology#KaryotypicalFemale
health-ri:KaryotypicalFemale rdf:type owl:Class ;
                             owl:equivalentClass [ rdf:type owl:Class ;
                                                   owl:unionOf ( health-ri:RegularKaryotypicalFemale
                                                                 health-ri:VariantKaryotypicalFemale
                                                               )
                                                 ] ;
                             rdfs:subClassOf health-ri:Person ;
                             owl:disjointWith health-ri:KaryotypicalMale .


###  https://w3id.org/health-ri/ontology#KaryotypicalMale
health-ri:KaryotypicalMale rdf:type owl:Class ;
                           owl:equivalentClass [ rdf:type owl:Class ;
                                                 owl:unionOf ( health-ri:RegularKaryotypicalMale
                                                               health-ri:VariantKaryotypicalMale
                                                             )
                                               ] ;
                           rdfs:subClassOf health-ri:Person .


###  https://w3id.org/health-ri/ontology#KaryotypicalSex
health-ri:KaryotypicalSex rdf:type owl:Class ;
                          rdfs:subClassOf gufo:IntrinsicMode ,
                                          health-ri:Sex ,
                                          [ rdf:type owl:Restriction ;
                                            owl:onProperty [ owl:inverseOf gufo:inheresIn
                                                           ] ;
                                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                            owl:onClass health-ri:Person
                                          ] ;
                          owl:disjointWith health-ri:PhenotypicSex .


###  https://w3id.org/health-ri/ontology#KaryotypicallyIndeterminatePerson
health-ri:KaryotypicallyIndeterminatePerson rdf:type owl:Class ;
                                            rdfs:subClassOf health-ri:PersonWithVariantSexChromosome .


###  https://w3id.org/health-ri/ontology#LegalFemalePerson
health-ri:LegalFemalePerson rdf:type owl:Class ;
                            rdfs:subClassOf health-ri:PersonWithRecognizedLegalGender .


###  https://w3id.org/health-ri/ontology#LegalGender
health-ri:LegalGender rdf:type owl:Class ;
                      rdfs:subClassOf health-ri:AdministrativeGender ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty [ owl:inverseOf gufo:inheresIn
                                                       ] ;
                                        owl:someValuesFrom health-ri:PersonWithRecognizedLegalGender
                                      ] .


###  https://w3id.org/health-ri/ontology#LegalGenderRecognition
health-ri:LegalGenderRecognition rdf:type owl:Class ;
                                 rdfs:subClassOf health-ri:AdministrativeGenderRecognition ,
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty gufo:mediates ;
                                                   owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                   owl:onClass health-ri:GenderRecognitionDocument
                                                 ] ,
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty gufo:mediates ;
                                                   owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                   owl:onClass health-ri:LegalGenderRecognizer
                                                 ] ,
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty gufo:mediates ;
                                                   owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                   owl:onClass health-ri:PersonWithRecognizedLegalGender
                                                 ] .


###  https://w3id.org/health-ri/ontology#LegalGenderRecognizer
health-ri:LegalGenderRecognizer rdf:type owl:Class ;
                                rdfs:subClassOf health-ri:AdministrativeGenderRecognizer ,
                                                health-ri:Government ,
                                                [ rdf:type owl:Restriction ;
                                                  owl:onProperty [ owl:inverseOf gufo:mediates
                                                                 ] ;
                                                  owl:someValuesFrom health-ri:LegalGenderRecognition
                                                ] .


###  https://w3id.org/health-ri/ontology#LegalMalePerson
health-ri:LegalMalePerson rdf:type owl:Class ;
                          rdfs:subClassOf health-ri:PersonWithRecognizedLegalGender .


###  https://w3id.org/health-ri/ontology#LegalNonBinaryPerson
health-ri:LegalNonBinaryPerson rdf:type owl:Class ;
                               rdfs:subClassOf health-ri:PersonWithRecognizedLegalGender .


###  https://w3id.org/health-ri/ontology#MaleBiologicalPerson
health-ri:MaleBiologicalPerson rdf:type owl:Class ;
                               rdfs:subClassOf health-ri:Person .


###  https://w3id.org/health-ri/ontology#MaleGenderIdentity
health-ri:MaleGenderIdentity rdf:type owl:Class ;
                             rdfs:subClassOf health-ri:GenderIdentity .


###  https://w3id.org/health-ri/ontology#MalePhenotypicPerson
health-ri:MalePhenotypicPerson rdf:type owl:Class ;
                               rdfs:subClassOf health-ri:PersonWithAssessedPhenotypicSex .


###  https://w3id.org/health-ri/ontology#MaleTypicalDimorphicCharacteristic
health-ri:MaleTypicalDimorphicCharacteristic rdf:type owl:Class ;
                                             rdfs:subClassOf health-ri:SexualDimorphicCharacteristic .


###  https://w3id.org/health-ri/ontology#MaleTypicalVisualSexCharacteristic
health-ri:MaleTypicalVisualSexCharacteristic rdf:type owl:Class ;
                                             rdfs:subClassOf health-ri:VisualSexCharacteristic .


###  https://w3id.org/health-ri/ontology#NonBinaryGenderIdentity
health-ri:NonBinaryGenderIdentity rdf:type owl:Class ;
                                  rdfs:subClassOf health-ri:GenderIdentity .


###  https://w3id.org/health-ri/ontology#Organization
health-ri:Organization rdf:type owl:Class ;
                       rdfs:subClassOf gufo:FunctionalComplex .


###  https://w3id.org/health-ri/ontology#Person
health-ri:Person rdf:type owl:Class ;
                 owl:equivalentClass [ rdf:type owl:Class ;
                                       owl:unionOf ( health-ri:FemaleBiologicalPerson
                                                     health-ri:IndeterminateBiologicalPerson
                                                     health-ri:MaleBiologicalPerson
                                                   )
                                     ] ,
                                     [ rdf:type owl:Class ;
                                       owl:unionOf ( health-ri:KaryotypicalFemale
                                                     health-ri:KaryotypicalMale
                                                   )
                                     ] ,
                                     [ rdf:type owl:Class ;
                                       owl:unionOf ( health-ri:PersonWithAssignedSexAtBirth
                                                     health-ri:PersonWithUnassignedSexAtBirth
                                                   )
                                     ] ,
                                     [ rdf:type owl:Class ;
                                       owl:unionOf ( health-ri:PersonWithRegularSexChromosome
                                                     health-ri:PersonWithVariantSexChromosome
                                                   )
                                     ] ;
                 rdfs:subClassOf gufo:FunctionalComplex ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty [ owl:inverseOf gufo:inheresIn
                                                  ] ;
                                   owl:someValuesFrom health-ri:Sex
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty gufo:inheresIn ;
                                   owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                   owl:onClass health-ri:KaryotypicalSex
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty [ owl:inverseOf gufo:inheresIn
                                                  ] ;
                                   owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                   owl:onClass health-ri:GenderIdentity
                                 ] .


###  https://w3id.org/health-ri/ontology#PersonWithAssessedPhenotypicSex
health-ri:PersonWithAssessedPhenotypicSex rdf:type owl:Class ;
                                          owl:equivalentClass [ rdf:type owl:Class ;
                                                                owl:unionOf ( health-ri:AmbiguousPhenotypicPerson
                                                                              health-ri:FemalePhenotypicPerson
                                                                              health-ri:MalePhenotypicPerson
                                                                            )
                                                              ] ;
                                          rdfs:subClassOf health-ri:Person ,
                                                          [ rdf:type owl:Restriction ;
                                                            owl:onProperty [ owl:inverseOf gufo:inheresIn
                                                                           ] ;
                                                            owl:someValuesFrom health-ri:PhenotypicSex
                                                          ] ,
                                                          [ rdf:type owl:Restriction ;
                                                            owl:onProperty [ owl:inverseOf gufo:mediates
                                                                           ] ;
                                                            owl:someValuesFrom health-ri:PhenotypicSexAssessment
                                                          ] .


###  https://w3id.org/health-ri/ontology#PersonWithAssignedSexAtBirth
health-ri:PersonWithAssignedSexAtBirth rdf:type owl:Class ;
                                       owl:equivalentClass [ rdf:type owl:Class ;
                                                             owl:unionOf ( health-ri:PersonWithFemaleSexAtBirth
                                                                           health-ri:PersonWithIndeterminateSexAtBirth
                                                                           health-ri:PersonWithMaleSexAtBirth
                                                                         )
                                                           ] ;
                                       rdfs:subClassOf health-ri:Person ,
                                                       [ rdf:type owl:Restriction ;
                                                         owl:onProperty [ owl:inverseOf gufo:mediates
                                                                        ] ;
                                                         owl:someValuesFrom health-ri:SexAtBirthAssignment
                                                       ] ,
                                                       [ rdf:type owl:Restriction ;
                                                         owl:onProperty [ owl:inverseOf gufo:inheresIn
                                                                        ] ;
                                                         owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                         owl:onClass health-ri:SexAtBirth
                                                       ] .


###  https://w3id.org/health-ri/ontology#PersonWithFemaleSexAtBirth
health-ri:PersonWithFemaleSexAtBirth rdf:type owl:Class ;
                                     rdfs:subClassOf health-ri:PersonWithAssignedSexAtBirth .


###  https://w3id.org/health-ri/ontology#PersonWithIndeterminateSexAtBirth
health-ri:PersonWithIndeterminateSexAtBirth rdf:type owl:Class ;
                                            rdfs:subClassOf health-ri:PersonWithAssignedSexAtBirth .


###  https://w3id.org/health-ri/ontology#PersonWithMaleSexAtBirth
health-ri:PersonWithMaleSexAtBirth rdf:type owl:Class ;
                                   rdfs:subClassOf health-ri:PersonWithAssignedSexAtBirth .


###  https://w3id.org/health-ri/ontology#PersonWithRecognizedAdministrativeGender
health-ri:PersonWithRecognizedAdministrativeGender rdf:type owl:Class ;
                                                   rdfs:subClassOf health-ri:Person ,
                                                                   [ rdf:type owl:Restriction ;
                                                                     owl:onProperty [ owl:inverseOf gufo:inheresIn
                                                                                    ] ;
                                                                     owl:someValuesFrom health-ri:AdministrativeGender
                                                                   ] ,
                                                                   [ rdf:type owl:Restriction ;
                                                                     owl:onProperty [ owl:inverseOf gufo:mediates
                                                                                    ] ;
                                                                     owl:someValuesFrom health-ri:AdministrativeGenderRecognition
                                                                   ] .


###  https://w3id.org/health-ri/ontology#PersonWithRecognizedLegalGender
health-ri:PersonWithRecognizedLegalGender rdf:type owl:Class ;
                                          owl:equivalentClass [ rdf:type owl:Class ;
                                                                owl:unionOf ( health-ri:LegalFemalePerson
                                                                              health-ri:LegalMalePerson
                                                                              health-ri:LegalNonBinaryPerson
                                                                              health-ri:PersonWithUnassignedLegalGender
                                                                            )
                                                              ] ;
                                          rdfs:subClassOf health-ri:Person ,
                                                          health-ri:PersonWithRecognizedAdministrativeGender ,
                                                          [ rdf:type owl:Restriction ;
                                                            owl:onProperty [ owl:inverseOf gufo:mediates
                                                                           ] ;
                                                            owl:someValuesFrom health-ri:LegalGenderRecognition
                                                          ] ,
                                                          [ rdf:type owl:Restriction ;
                                                            owl:onProperty gufo:inheresIn ;
                                                            owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                            owl:onClass health-ri:LegalGender
                                                          ] .


###  https://w3id.org/health-ri/ontology#PersonWithRegularSexChromosome
health-ri:PersonWithRegularSexChromosome rdf:type owl:Class ;
                                         owl:equivalentClass [ rdf:type owl:Class ;
                                                               owl:unionOf ( health-ri:RegularKaryotypicalFemale
                                                                             health-ri:RegularKaryotypicalMale
                                                                           )
                                                             ] ;
                                         rdfs:subClassOf health-ri:Person ;
                                         owl:disjointWith health-ri:PersonWithVariantSexChromosome .


###  https://w3id.org/health-ri/ontology#PersonWithUnassignedLegalGender
health-ri:PersonWithUnassignedLegalGender rdf:type owl:Class ;
                                          rdfs:subClassOf health-ri:PersonWithRecognizedLegalGender .


###  https://w3id.org/health-ri/ontology#PersonWithUnassignedSexAtBirth
health-ri:PersonWithUnassignedSexAtBirth rdf:type owl:Class ;
                                         rdfs:subClassOf health-ri:Person .


###  https://w3id.org/health-ri/ontology#PersonWithVariantSexChromosome
health-ri:PersonWithVariantSexChromosome rdf:type owl:Class ;
                                         owl:equivalentClass [ rdf:type owl:Class ;
                                                               owl:unionOf ( health-ri:KaryotypicallyIndeterminatePerson
                                                                             health-ri:VariantKaryotypicalFemale
                                                                             health-ri:VariantKaryotypicalMale
                                                                           )
                                                             ] ;
                                         rdfs:subClassOf health-ri:Person .


###  https://w3id.org/health-ri/ontology#PhenotypicSex
health-ri:PhenotypicSex rdf:type owl:Class ;
                        rdfs:subClassOf gufo:ExtrinsicMode ,
                                        health-ri:Sex ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty gufo:inheresIn ;
                                          owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                          owl:onClass health-ri:PersonWithAssessedPhenotypicSex
                                        ] .


###  https://w3id.org/health-ri/ontology#PhenotypicSexAssessment
health-ri:PhenotypicSexAssessment rdf:type owl:Class ;
                                  rdfs:subClassOf gufo:Relator ,
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty [ owl:inverseOf gufo:mediates
                                                                   ] ;
                                                    owl:someValuesFrom health-ri:PhenotypicSexAssessor
                                                  ] ,
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty gufo:mediates ;
                                                    owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                    owl:onClass health-ri:PersonWithAssessedPhenotypicSex
                                                  ] .


