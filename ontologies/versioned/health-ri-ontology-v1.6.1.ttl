    rdfs:isDefinedBy <https://w3id.org/health-ri/ontology> ;
    vs:term_status "erv" ;
    skos:prefLabel "Gender"@en .

hrio:personIsComponentOfVisualSexCharacteristic rdfs:isDefinedBy <https://w3id.org/health-ri/ontology> .

hrio:person_2 rdfs:isDefinedBy <https://w3id.org/health-ri/ontology> .

